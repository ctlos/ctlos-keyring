post_install() {
  pacman-key --init
  pacman-key --add /usr/share/pacman/keyrings/ctlos.gpg
  pacman-key --lsign-key 50417293016B25BED7249D8398F76D97B786E6A3
  pacman-key --populate
}

post_upgrade() {
  pacman-key --init
  pacman-key --add /usr/share/pacman/keyrings/ctlos.gpg
  pacman-key --lsign-key 50417293016B25BED7249D8398F76D97B786E6A3
  pacman-key --populate
}
